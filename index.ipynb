    "import typing\n",
    "\n",
    "# training function to turn a model into a trained network\n",
    "trainTemplate = \"epoch: {} test loss: {:.3f} test accuracy: {:.3f}\"\n",
    "testTemplate = \"epoch: {} test loss: {:.3f} test accuracy: {:.3f}\"\n",
    "\n",
    "\n",
    "def train_network_from_model(model, train_x, train_y, test_x, test_y, train_settings: TrainSettings = TrainSettings()):\n",
    "    print(f\"using {train_settings.DEVICE} for training, EPOCHS={train_settings.EPOCHS}, BATCH_SIZE={train_settings.BATCH_SIZE}, LR={train_settings.LR}\")\n",
    "    \n",
    "    network = model.to(train_settings.DEVICE)\n",
    "    optimizer = SGD(network.parameters(), lr=train_settings.LR)\n",
    "\n",
    "    for epoch in range(0, train_settings.EPOCHS):\n",
    "        \n",
    "        loss, acc, samples = train_network(network, optimizer, train_x, train_y, train_settings)\n",
    "        print(\"epoch: {} train loss: {:.3f} train accuracy: {:.3f}\".format(epoch + 1, (loss / samples),\n",
    "            (acc / samples)))\n",
    "\n",
    "        loss, acc = evaluate_network(network, test_x, test_y, train_settings)\n",
    "        print(\"epoch: {} test loss: {:.3f} test accuracy: {:.3f}\".format(epoch+1, loss, acc))\n",
    "    return network\n",
    "\n",
    "def train_network(network, optimizer, train_x, train_y, train_settings: TrainSettings):\n",
    "    loss, acc, samples = 0,0,0\n",
    "    network.train()\n",
    "    for (bx,by) in batch_iterator(train_x, train_y, train_settings.BATCH_SIZE):\n",
    "        l, a, s = train_network_on_batch(network, optimizer, bx,by, train_settings)\n",
    "        loss += l\n",
    "        acc += a\n",
    "        samples += s\n",
    "    return loss, acc, samples\n",
    "\n",
    "def train_network_on_batch(network, optimizer, bx,by,train_settings: TrainSettings):\n",
    "    pred = network(bx)\n",
    "    loss = train_settings.LOSS_FUNCTION(pred, by.float())\n",
    "    optimizer.zero_grad()\n",
    "    loss.backward()\n",
    "    optimizer.step()\n",
    "    loss = loss.item() * by.size(0)\n",
    "    acc = (pred.max(1)[1] == by).sum().item()\n",
    "    samples = by.size(0)\n",
    "    return loss, acc, samples\n",
    "\n",
    "def evaluate_network(network, test_x, test_y, train_settings):\n",
    "    testLoss = 0\n",
    "    testAcc = 0\n",
    "    samples = 0\n",
    "    network.eval() # activate eval mode\n",
    "    # initialize a no-gradient context\n",
    "    with torch.no_grad():\n",
    "        # loop over the current batch of test data\n",
    "        (test_x_1, test_y_1) = (test_x.to(train_settings.DEVICE), test_y.to(train_settings.DEVICE))\n",
    "        predictions = network(test_x_1).float()\n",
    "        loss = train_settings.LOSS_FUNCTION(predictions, test_y_1.float())\n",
    "        acc = (predictions.max(1)[1] == test_y_1).sum().item()\n",
    "        print(f\"loss: {loss}\")\n",
    "        print(f\"acc: {acc}\")\n",
    "        samples = test_y_1.size(0)\n",
    "        \n",
    "        # (loss, accuracy)\n",
    "        return(loss / samples, acc / samples)\n",
    "\n"
