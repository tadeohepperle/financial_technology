    "def determine_device():\n",
    "    DEVICE = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n",
    "    return DEVICE\n",
    "\n",
    "class TrainSettings:\n",
    "    EPOCHS = 2\n",
    "    BATCH_SIZE = 64\n",
    "    LR = 0.01\n",
    "    LOSS_FUNCTION = nn.BCELoss()\n",
    "    DEVICE = determine_device()\n",
